{
  "name": "ski",
  "version": "0.0.0",
  "author": "Matt Tingen <matt.tingen@gmail.com>",
  "scripts": {
    "build": "tsc && VITE_DEPLOY_URL=$DEPLOY_URL vite build",
    "format": "prettier --ignore-path .gitignore --write src",
    "lint": "eslint src",
    "start": "VITE_DEPLOY_URL=$DEPLOY_URL vite --host",
    "test": "pnpm run build && npx playwright test",
    "test:integration-update": "npx playwright test --update-snapshots",
    "ts": "tsc --watch"
  },
  "dependencies": {
    "@matt-tingen/util": "^3.1.0",
    "@netlify/functions": "^2.8.2",
    "dedent": "1.5.3",
    "es-toolkit": "1.29.0",
    "excalibur": "0.30.1",
    "nes.css": "2.3.0",
    "seed-random": "2.2.0"
  },
  "devDependencies": {
    "@matt-tingen/eslint-config": "0.10.0-rc1",
    "@playwright/test": "^1.49.0",
    "@types/node": "^22.10.0",
    "@types/seed-random": "2.2.4",
    "eslint": "^8.57.0",
    "prettier": "^3.4.2",
    "prettier-plugin-packagejson": "^2.5.6",
    "typescript": "5.7.2",
    "vite": "6.0.3",
    "wrangler": "3.95.0"
  }
}
